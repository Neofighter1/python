n=input()
j=0
for i in n:
   j=j+int(i)
if j==1:print("Magic No.")
elif j<=9:print("not")
else:
    f=0
    for i in str(j):
       f=f+int(i)
    if f==1:print("Magic No.")
    elif f<=9:print("not")